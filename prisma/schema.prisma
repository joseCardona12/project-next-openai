generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users{
  id            Int         @id @default(autoincrement())
  name          String
  emial         String      @unique
  password      String    
  gender_id     Int         
  gender        Genders     @relation(fields: [gender_id], references: [id])
  createdAt     DateTime    @default(now())
}

model Genders{
  id            Int          @id @default(autoincrement())
  name          String       @unique
  users         Users[]
}

